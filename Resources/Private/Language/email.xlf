			<trans-unit id="reset_password.button_reset">
				<source>Reset my password</source>
			</trans-unit>
			<trans-unit id="reset_password.expire">
				<source>If you haven't made this request, no further action required. It will be expired in %s minutes.</source>
			</trans-unit>



			<trans-unit id="update_password.subject">
				<source>Security Notice: Password has been changed</source>
			</trans-unit>
			<trans-unit id="update_password.caption">
