{
    "repositories": [
        {"type": "composer", "url": "https://composer.typo3.org/"},
        {"type": "git", "url": "https://gitlab.com/borulko.sergey/routes.git"},
        {"type": "git", "url": "https://github.com/benjaminkott/site-introduction.git"}
    ],
    "name": "test/login",
    "description": "Testing sandbox for extensions",
    "license": "GPL-2.0-or-later",
    "require": {
        "bk2k/introduction-website": "dev-master",

        "lms/routes": "~1.8",
        "lms/facade": "dev-master",
        "lms/login": "dev-master"
    },
    "require-dev": {
		"vimeo/psalm": "^3",
		"mockery/mockery": "^1",
        "typo3/testing-framework": "^4",
        "nimut/testing-framework": "^4",
        "codeception/codeception": "^3.1.0",
        "ericmartel/codeception-email-mailhog": "^1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "LMS\\Login\\Tests\\": "public/typo3conf/ext/login/Tests"
        }
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin",
        "vendor-dir": "vendor"
    }
}
