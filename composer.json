{
	"name": "lms/login",
	"type": "typo3-cms-extension",
	"description": "Provides an authentication option for website users.",
	"license": "MIT",
	"authors": [
		{
			"name": "Serhii Borulko",
			"email": "borulkosergey@icloud.com"
		}
	],
	"require": {
		"typo3/cms-core": "^9.5",
		"lms3/support": "*"
	},
	"require-dev": {
		"nimut/testing-framework": "^4",
		"codeception/codeception": "^4",
		"ericmartel/codeception-email-mailhog": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"LMS\\Login\\": "Classes"
		},
		"autoload-dev": {
			"psr-4": {
				"LMS\\Login\\Tests\\": "Tests"
			}
		}
	},
	"config": {
		"platform": {
			"php": "7.2"
		}
	},
	"extra": {
		"typo3/cms": {
			"app-dir": "app",
			"root-dir": "app",
			"web-dir": "web"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
